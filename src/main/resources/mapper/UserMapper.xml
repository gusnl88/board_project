<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.project.board_project.mapper.UserMapper">
    <resultMap id="UserMapper" type="UserDto">
        <id column="u_id" property="uId"/>
        <result column="pw" property="pw"/>
        <result column="name" property="name"/>
    </resultMap>
    <insert id="insertOne" parameterType="com.project.board_project.DTO.UserDto">
        INSERT INTO users
        (u_id,pw,name)
        VALUES
        (#{uId},#{pw},#{name})
    </insert>
    <select id="findByUserId" resultMap="UserMapper">
        SELECT * FROM users WHERE u_id=#{uId}
    </select>
</mapper>